  <systemMessage>&lt;H3&gt;OVERVIEW:&lt;/H3&gt; &#xd;
&lt;B&gt; To Execute JON 23 Tests, click the JON23 tab &lt;/B&gt;&lt;br&gt;&#xd;
&lt;B&gt; To Execute RHQ (iManage) Tests, click the RHQ tab &lt;/B&gt;&lt;br&gt;&#xd;
&#xd;
</systemMessage>
  <jdks>
    <jdk>
      <name>Open JDK 1.6</name>
      <home>/usr/lib/jvm/java-1.6.0-openjdk-1.6.0.0.x86_64</home>
      <properties/>
    </jdk>
  </jdks>
  <clouds/>
  <slaves>
    <slave>
      <name>jweiss.usersys.redhat.com</name>
      <description>Jeff Weiss&apos;s test slave</description>
      <remoteFS>/home/hudson</remoteFS>
      <numExecutors>1</numExecutors>
      <mode>EXCLUSIVE</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.slaves.JNLPLauncher"/>
      <label></label>
      <nodeProperties>
        <hudson.tools.ToolLocationNodeProperty>
          <locations>
            <hudson.tools.ToolLocationNodeProperty_-ToolLocation>
              <type>hudson.tasks.Maven$MavenInstallation$DescriptorImpl</type>
