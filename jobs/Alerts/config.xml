<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>&lt;H3&gt;INSTRUCTIONS:&lt;/H3&gt; &#xd;
&lt;UL&gt;&#xd;
&lt;LI&gt; Click &apos;Build Now&apos; to begin a test run. &lt;/LI&gt;&#xd;
&lt;LI&gt; From the list of fields, read their descriptions and set the values appropriate for your testing. &lt;/LI&gt;&#xd;
&lt;LI&gt; Then click the &apos;Build&apos; button. &lt;/LI&gt;&#xd;
&lt;LI&gt; View your tests in live action by typing: &lt;B&gt;&lt;CODE&gt;vncviewer statler.usersys.redhat.com:0&lt;/CODE&gt;&lt;/B&gt; &lt;/LI&gt;&#xd;
&lt;/UL&gt;&#xd;
&#xd;
&#xd;
</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.security.AuthorizationMatrixProperty>
      <permission>hudson.model.Item.Workspace:anonymous</permission>
      <permission>hudson.model.Item.Read:anonymous</permission>
    </hudson.security.AuthorizationMatrixProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>TEST_GROUP</name>
          <description>&lt;b&gt;&lt;big&gt;The testng group name&lt;/big&gt;&lt;/b&gt;</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>Alerts</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SERVER_URL</name>
          <description>&lt;b&gt;&lt;big&gt;URL to your RHQ server: http://yourserver.com:7080&lt;/big&gt;&lt;/b&gt;</description>
          <defaultValue>http://auto-rhq.usersys.redhat.com:7080</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>VERSION</name>
          <description>&lt;b&gt;&lt;big&gt;1.4.0 is our version number for RHQ (iManage)&lt;/big&gt;&lt;/b&gt;</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>1.4.0</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>USERNAME</name>
          <description>&lt;b&gt;&lt;big&gt;Administrator for RHQ (iManage)&lt;/big&gt;&lt;/b&gt;</description>
          <defaultValue>rhqadmin</defaultValue>
        </hudson.model.StringParameterDefinition>
        <hudson.model.PasswordParameterDefinition>
          <name>PASSWORD</name>
          <description>&lt;b&gt;&lt;big&gt;Password for admin of RHQ (iManage)&lt;/big&gt;&lt;/b&gt;</description>
          <defaultValue>rhqadmin</defaultValue>
        </hudson.model.PasswordParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>SERVER_USER</name>
          <description>&lt;b&gt;&lt;big&gt;User running your rhq-server process&lt;/big&gt;&lt;/b&gt;&lt;br&gt;
&lt;b&gt;&lt;big&gt;Common users: &quot;root&quot;, &quot;jonqa&quot;, &quot;rhq&quot;&lt;/big&gt;&lt;/b&gt;
</description>
          <defaultValue>rhq</defaultValue>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>echo &quot;==================&quot;
echo  ${TEST_GROUP}
echo &quot;==================&quot;
rm -Rf *
git clone git://github.com/weissjeffm/webui-framework.git
git clone git://git.rhndev.redhat.com/automatjon
chmod 600 $WORKSPACE/automatjon/jon-2.0/.ssh/id*</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Ant>
      <targets>compile-jon</targets>
      <antName>(Default)</antName>
      <buildFile>automatjon/jon-2.0/build.xml</buildFile>
    </hudson.tasks.Ant>
    <hudson.plugins.groovy.Groovy>
      <scriptSource class="hudson.plugins.groovy.FileScriptSource">
        <scriptFile>automatjon/jon-2.0/src/hudson.groovy</scriptFile>
      </scriptSource>
      <groovyName>(Default)</groovyName>
      <parameters>-Dhudson.test.name=$TEST_GROUP   -Djon.server.url=$SERVER_URL -Djon.version=$VERSION  -Djon.admin.user=$USERNAME -Djon.admin.password=$PASSWORD -Djon.server.user=$SERVER_USER -Dtestng.outputdir=automatjon/jon-2.0/test-output</parameters>
      <scriptParameters>automatjon/jon-2.0/src/com/redhat/qe/rhq14/tests/functional-testng.xml $TEST_GROUP</scriptParameters>
      <properties></properties>
    </hudson.plugins.groovy.Groovy>
    <hudson.tasks.Ant>
      <targets>reports</targets>
      <antName>(Default)</antName>
      <buildFile>automatjon/jon-2.0/build.xml</buildFile>
    </hudson.tasks.Ant>
    <hudson.tasks.Shell>
      <command>#kill all the leftover firefox processes that selenium started
sudo -u vncuser killall -9 firefox || echo &quot;looks like firefox is already gone.&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>automatjon/jon-2.0/test-output/**/*</artifacts>
      <latestOnly>false</latestOnly>
    </hudson.tasks.ArtifactArchiver>
    <hudson.tasks.junit.JUnitResultArchiver>
      <testResults>automatjon/jon-2.0/test-output-reportng/*.xml</testResults>
      <testDataPublishers/>
    </hudson.tasks.junit.JUnitResultArchiver>
    <htmlpublisher.HtmlPublisher>
      <reportTargets>
        <htmlpublisher.HtmlPublisherTarget>
          <reportName>TestNG Report</reportName>
          <reportDir>automatjon/jon-2.0/test-output/html/</reportDir>
          <reportFiles>index.html</reportFiles>
          <keepAll>true</keepAll>
          <wrapperName>htmlpublisher-wrapper.html</wrapperName>
        </htmlpublisher.HtmlPublisherTarget>
      </reportTargets>
    </htmlpublisher.HtmlPublisher>
  </publishers>
  <buildWrappers/>
</project>